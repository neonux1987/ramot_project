{
  "name": "ramot-group-income-outcome-management",
  "version": "1.0.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/neonux1987/ramot_project.git"
  },
  "author": {
    "name": "Andrey Glemb",
    "email": "neonux1987@gmail.com",
    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.my-website.ramot-group-income-outcome-management",
    "productName": "קבוצת רמות ניהול הוצאות והכנסות",
    "copyright": "Copyright © 2019 ${author}",
    "extends": null,
    "files": [
      "build/**/*",
      "package.json",
      "!dev-app-update.yml",
      "electron/**/*"
    ],
    "extraResources": [
      "./extraResources/**"
    ],
    "asarUnpack": [
      "node_modules/highcharts-export-server/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./Icon/ramot-group-icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "neonux1987",
        "private": true,
        "releaseType": "release",
        "protocol": "https",
        "repo": "ramot_project"
      }
    ],
    "nsis": {
      "allowToChangeInstallationDirectory": false,
      "oneClick": true,
      "createDesktopShortcut": true,
      "deleteAppDataOnUninstall": true,
      "differentialPackage": false
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && nodemon --exec electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win --publish never",
    "electron-deploy": "npm run build && electron-builder --win --publish always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "electron-context-menu": "^2.3.0",
    "electron-log": "^4.3.4",
    "electron-updater": "^4.6.0",
    "exceljs": "^4.2.0",
    "file-type": "^16.0.0",
    "fs-extra": "^9.0.1",
    "highcharts": "^9.0.1",
    "highcharts-export-server": "^2.1.0",
    "knex": "^0.21.11",
    "nanoid": "^3.1.22",
    "nanoid-dictionary": "^4.3.0",
    "nesthydrationjs": "^2.0.0",
    "pdf-lib": "^1.16.0",
    "platform-folders": "^0.5.2",
    "sqlite3": "^5.0.0",
    "v8-compile-cache": "^2.3.0"
  },
  "devDependencies": {
    "@iconify/react": "^3.2.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@rescripts/cli": "0.0.14",
    "@rescripts/rescript-env": "0.0.12",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "13.1.4",
    "electron-builder": "^22.11.7",
    "emotion": "^10.0.27",
    "highcharts-react-official": "^3.0.0",
    "jss-rtl": "^0.3.0",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-error-overlay": "^6.0.9",
    "react-goodbye": "^0.4.0",
    "react-icons": "^4.2.0",
    "react-jss": "^10.4.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-select": "^3.1.0",
    "react-spinners": "^0.9.0",
    "react-toastify": "^5.5.0",
    "react-virtuoso": "^1.6.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "source-map-explorer": "^2.5.2",
    "wait-on": "^5.2.1"
  }
}
