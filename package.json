{
  "name": "ramot-mezach-dohot",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/neonux1987/financial_reports_management_system.git"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@material-ui/styles": "^4.7.1",
    "date-fns": "^2.8.1",
    "electron-context-menu": "^0.15.1",
    "electron-is-dev": "^1.1.0",
    "electron-updater": "^4.3.1",
    "event-emitter": "^0.3.5",
    "exceljs": "^3.8.2",
    "fs-extra": "^9.0.0",
    "jss-rtl": "^0.3.0",
    "knex": "^0.20.13",
    "nesthydrationjs": "^2.0.0",
    "node-schedule": "^1.3.2",
    "platform-folders": "^0.5.1",
    "print-js": "^1.0.63",
    "react": "^16.13.1",
    "react-collapsible": "^2.7.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.8.0",
    "react-jss": "^10.0.0",
    "react-portal": "^4.2.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-scroll": "^1.7.15",
    "react-select": "^3.0.8",
    "react-slick": "^0.25.2",
    "react-spinners": "^0.6.1",
    "react-textarea-autosize": "^7.1.2",
    "react-toast-notifications": "^2.4.0",
    "react-toastify": "^5.4.1",
    "react-virtuoso": "^0.12.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "scheduler": "^0.19.1",
    "slick-carousel": "^1.8.1",
    "sqlite3": "^4.1.1",
    "typeface-roboto": "0.0.75"
  },
  "author": {
    "name": "Andrey Glemb",
    "email": "neonux1987@gmail.com",
    "url": "https://your-website.com"
  },
  "build": {
    "appId": "com.my-website.ramot-mezach-dohot",
    "productName": "רמות מזח דוחות",
    "copyright": "Copyright © 2019 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      "./extraResources/**"
    ],
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Development"
    },
    "win": {
      "target": [
        "zip"
      ],
      "publish": [
        "github"
      ]
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder --win --publish never",
    "electron-deploy": "electron-builder --win --publish always"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": []
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.13",
    "@rescripts/rescript-env": "0.0.11",
    "concurrently": "^5.0.1",
    "cross-env": "^6.0.3",
    "electron": "7.1.4",
    "electron-builder": "^21.2.0",
    "typescript": "^3.7.3",
    "wait-on": "^3.3.0"
  }
}
